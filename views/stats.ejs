<!DOCTYPE html>
<html lang="en">
  <head>
    <% include partials/head %>
  </head>
  <body>
    <% include partials/nav %>
    <h1><%= title %></h1>
    <p>This is where the stats will go, and there will be a counter.</p>

    <% stats.forEach(function(stat) { %> 
      <% if (stat.number === "one") { %>
        <% oneArr.push(stat.number); %>
      <% } else if (stat.number === "two") { %>
        <% twoArr.push(stat.number); %>
      <% } else if (stat.number === "three") { %>
        <% threeArr.push(stat.number); %>
      <% } else if (stat.number === "four") { %>
        <% fourArr.push(stat.number); %>
      <% } else if (stat.number === "five") { %>
        <% fiveArr.push(stat.number); %>
      <% } %>
    <% }); %>
    <div id="stats-container">
      <div class="stats">
        <p>Ones:<br><%= oneArr.length %></p>
      </div>
      <div class="stats">
        <p>Twos:<br><%= twoArr.length %></p>
      </div>
      <div class="stats">
        <p>Threes:<br><%= threeArr.length %></p>
      </div>
      <div class="stats">
        <p>Fours:<br><%= fourArr.length %></p> 
      </div>
      <div class="stats">
        <p>Fives:<br><%= fiveArr.length %></p>
      </div>
    </div>
  </body>
</html>